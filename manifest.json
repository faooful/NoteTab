{
  "manifest_version": 3,

  "name": "Notes Tab",
  "description": "A new tab replacement for taking notes",
  "version": "1.0",

  "permissions": [
    "storage"
  ],

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
